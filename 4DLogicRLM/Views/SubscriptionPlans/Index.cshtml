@model IEnumerable<_4DLogicRLM.Models.SubscriptionPlans>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.SubscriptionPlanName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SubscriptionPlanDescription)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SubscriptionPlanFee)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SubscriptionUserCount)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.SubscriptionPlanName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SubscriptionPlanDescription)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SubscriptionPlanFee)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SubscriptionUserCount)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.SubscriptionPlanID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.SubscriptionPlanID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.SubscriptionPlanID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
